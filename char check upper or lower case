import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner alpha = new Scanner(System.in);
        char ch = alpha.next().charAt(0);
        if(ch>='A' && ch<='Z'){
            System.out.println(1);
            
        }else if(ch>='a' && ch<='z'){
            System.out.println(0);
        }else
            System.out.println(-1);

    }
}
